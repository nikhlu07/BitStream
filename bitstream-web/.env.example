# Turnkey Configuration
# You need to get these values from your Turnkey dashboard

# Your Turnkey Organization ID (required)
VITE_TURNKEY_ORGANIZATION_ID=your-org-id-here

# Your Turnkey Auth Proxy Config ID (required)
VITE_TURNKEY_AUTH_PROXY_CONFIG_ID=your-auth-proxy-config-id-here

# Turnkey API Base URL (usually this default is fine)
VITE_TURNKEY_API_BASE_URL=https://api.turnkey.com

# Your domain for passkey authentication (usually localhost for development)
VITE_TURNKEY_RP_ID=localhost

# Demo mode - set to 'false' for real transactions
VITE_DEMO_MODE=false

# Enable testnet faucet button
VITE_ENABLE_FAUCET=true
